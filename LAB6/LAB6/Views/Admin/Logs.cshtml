@model LAB6.Models.Admin.LogModel

<h1> Logs </h1>
@foreach (var log in Model.logs)
{
    <hr>
    <table class="table table-hover table-dark">
        <tbody>
        <tr>
            <th scope="row">ID</th>
            <td>@log.id</td>
        </tr>
        <tr>
            <th scope="row">Client ID</th>
            <td>@log.clientId</td>
        </tr>
        <tr>
            <th scope="row">Happened At</th>
            <td>@log.happenedAt</td>
        </tr>
        <tr>
            <th scope="row">Type</th>
            <td>@log.logType</td>
        </tr>
        </tbody>
    </table>
    <hr>
}